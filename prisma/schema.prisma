generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model cupons {
  cupom_id Int @id @default(autoincrement())
  cupom_nome String
  cupom_codigo String
  cupom_validade DateTime
  createdAt DateTime @default(now())
  updateAT DateTime @updatedAt
}